                        <!-- Modal d'ajout de fichiers au mail -->
                        <div class="modal fade" id="uploadQuoteRequestFileModal" tabindex="-1" role="dialog"
                             aria-labelledby="Upload Picture" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content">
                                    <form id="formAddQuoteRequestFile" name="formAddQuoteRequestFile"
                                          action="{{ path("paprec_quote_request_add_quote_request_file", {quoteRequestId: quoteRequest.id}) }}"
                                          method="post" enctype="multipart/form-data">
                                        <div class="modal-header">
                                            <h5 class="modal-title">
                                                {{ 'Commercial.QuoteRequestFile.AddTitle'| trans({}, "messages", app.request.locale) }}
                                            </h5>
                                            <button type="button" class="close" data-dismiss="modal"
                                                    aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="form-group">
                                                <div class="form-label">
                                                    <span>{{ 'Commercial.QuoteRequestFile.MaxSize'| trans({}, "messages", app.request.locale) }}</span>
                                                </div>
                                                <div class="form-content">
                                                    {{ form_widget(formAddQuoteRequestFile.systemPath, {'attr': {'class': 'form-control'}}) }}
                                                    {{ form_errors(formAddQuoteRequestFile.systemPath) }}
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="form-label">
                                                    <span>{{ 'Commercial.QuoteRequestFile.Type.Type'| trans({}, "messages", app.request.locale) }}</span>
                                                </div>
                                                <div class="form-content">
                                                    {{ form_widget(formAddQuoteRequestFile.type, {'attr': {'class': 'form-control'}}) }}
                                                    {{ form_errors(formAddQuoteRequestFile.type) }}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-sm btn-secondary"
                                                    data-dismiss="modal">
                                                {{ 'General.Cancel'| trans({}, "messages", app.request.locale) }}
                                            </button>
                                            <button type="button" class="btn btn-sm btn-primary"
                                                    id="formAddQuoteRequestFileSubmitButton">
                                                {{ 'General.Save'| trans({}, "messages", app.request.locale) }}
                                            </button>
                                        </div>
                                        {{ form_rest(formAddQuoteRequestFile) }}
                                    </form>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="float-left">
                        <a href="{{ path('paprec_quoteRequest_index') }}" class="btn btn-sm btn-default"><i
                                    class="fa fa-xs fa-chevron-left"></i> {{ 'Commercial.QuoteRequest.List'|trans }}</a>
                    </div>
                </div>
                <div class="card-body">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title">{{ 'Commercial.QuoteRequest.Informations-FO'|trans }}</h5>
                            </div>
                            <div class="card-body row">
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Catalog.Catalog'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.catalog %} {{ ('Commercial.QuoteRequest.Catalog.'~ quoteRequest.catalog)|trans }}{% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Origin.Origin'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.origin %} {{ ('Commercial.QuoteRequest.Origin.'~ quoteRequest.origin)|trans }}{% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Number'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.number }}
                                        </label>
                                    </div>

                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Locale'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.locale }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.BusinessName'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.businessName }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Civility'|trans }}</label>
                                        {% if quoteRequest.civility is not null %}
                                            <label class="col-form-label">
                                                {{ ('General.' ~ quoteRequest.civility)|trans }}
                                            </label>
                                        {% endif %}
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.LastName'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.lastName }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.FirstName'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.firstName }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Email'|trans }}</label>
                                        <label class="col-form-label">
                                            <a class="send-generated-contract-button" href="javascript:void(0);">
                                                {{ quoteRequest.email }}
                                            </a>
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Phone'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.phone }}
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Public.Contact.Access'| trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.access %}
                                                {{ ('Commercial.AccessList.' ~ quoteRequest.access)|trans }}
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Public.Contact.FloorNumber'| trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.floorNumber }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Public.Contact.Staff'| trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.staff %}
                                                {{ ('Commercial.StaffList.' ~ quoteRequest.staff)|trans }}
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.PonctualDate'|trans }}</label>
                                        {% if quoteRequest.ponctualDate is not null and quoteRequest.catalog == 'PONCTUAL' %}
                                            <label class="col-form-label">
                                                {{ quoteRequest.ponctualDate|localizeddate('short', 'none', app.request.locale) }}
                                            </label>
                                        {% endif %}
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.IsMultisite'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ ('General.' ~ (quoteRequest.isMultisite ? 'Yes' : 'No'))|trans }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Address'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.address }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.PostalCode'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.postalCode %}
                                                <a href="{{ path('paprec_postalCode_view', {id: quoteRequest.postalCode.id}) }}">
                                                    {{ quoteRequest.postalCode.code }}
                                                </a>
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.City'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.city }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.BillingAddress'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.billingAddress }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.BillingPostalCode'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.billingPostalCode %}
                                                {% if quoteRequest.isSameAddress == 1 %}
                                                    <a href="{{ path('paprec_postalCode_view', {id: quoteRequest.postalCode.id}) }}">
                                                        {{ quoteRequest.billingPostalCode }}
                                                    </a>
                                                {% else %}
                                                    {{ quoteRequest.billingPostalCode }}
                                                {% endif %}
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.BillingCity'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.billingCity }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.Comment'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.comment }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Public.Catalog.Other-Needs'| trans }}</label>
                                        {% if quoteRequest.otherNeeds and quoteRequest.otherNeeds|length > 0 %}
                                            <ul class="col-form-label">
                                                {% for otherNeed in quoteRequest.otherNeeds %}
                                                    <li class="contact-other-need">- {{ otherNeed.name }}</li>
                                                {% endfor %}
                                            </ul>
                                        {% else %}
                                            <label class="col-form-label">N/A</label>
                                        {% endif %}
                                    </div>
                                </div>
                                <div class="col-md-6 mt-4">
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.SignatoryFirstName1'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.signatoryFirstName1 }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.SignatoryLastName1'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.signatoryLastName1 }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.SignatoryTitle1'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ quoteRequest.signatoryTitle1 }}
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 mt-3">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title">{{ 'Commercial.QuoteRequest.Informations-BO'|trans }}</h5>
                            </div>
                            <div class="card-body row">
                                <div class="col-md-6">
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.QuoteStatus'|trans }}</label>
                                        <label class="col-form-label">
                                            {{ ('Commercial.QuoteStatusList.' ~ quoteRequest.quoteStatus)|trans }}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.RegularDuration'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.duration is null %}
                                                0 {{ 'Commercial.QuoteRequest.MONTH'|trans }}
                                            {% else %}
                                                {{ quoteRequest.duration }} {{ 'Commercial.QuoteRequest.MONTH'|trans }}
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.StartDate'|trans }}</label>
                                        <label class="col-form-label">
                                            {% if quoteRequest.startDate is not null %}
                                                {{ quoteRequest.startDate|localizeddate('short', 'none', app.request.locale) }}
                                            {% endif %}
                                        </label>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-4 col-form-label">{{ 'Commercial.QuoteRequest.CustomerId'|trans }}</label>
                                        <label class="col-form-label">
