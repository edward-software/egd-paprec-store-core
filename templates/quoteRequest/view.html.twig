                        url: "{{ path('paprec_follow_up_edit', {id: '__rowId__'}) }}"
                    },
                    {
                        label: '{{ "General.Remove"|trans }}',
                        type: 'ajax',
                        url: "{{ path("paprec_follow_up_removeMany", {ids: '__rowId__', json: 1}) }}",
                        confirmMessage: "<span class='confirm-remove-style'>{{ 'General.Confirm'|trans|e('js') }}</span><br>{{ 'General.Confirm-remove'|trans|e('js') }}",
                        className: 'btn btn-danger btn-sm',
                        icon: "fa fa-times"
                    }

                ]
            });

            $('#commmecialQuoteRequestRemoveButton').on('click', function () {

                {% if is_granted('ROLE_ADMIN') %}

                var url = "{{ path("paprec_quoteRequest_remove", {id: quoteRequest.id}) }}";

                Swal.fire({
                    title: "{{ 'General.Confirm'|trans }}",
                    text: "{{ 'General.Confirm-remove'|trans }}",
                    showCancelButton: true,
                    confirmButtonText: "{{ 'General.Yes'|trans }}",
                    cancelButtonText: "{{ 'General.No'|trans }}"
                }).then(function (result) {
                    if (result.value) {
                        $(location).attr('href', url);
                    }
                });
                {% else %}
                Swal.fire({
                    title: "{{ 'General.CantRemove'|trans }}",
                    text: "",
                    showCancelButton: false,
                    confirmButtonText: "{{ 'General.Cancel'|trans }}"
                });
                {% endif %}


            });
            $('#commmecialQuoteRequestMissionSheetValidateButton').on('click', function () {

                {% if quoteRequest and quoteRequest.missionSheet %}
                var url = "{{ path("paprec_quote_request_mission_sheet_validate", {id: quoteRequest.id, missionSheetId: quoteRequest.missionSheet.id}) }}";

                Swal.fire({
                    title: "{{ 'General.Confirm'|trans }}",
                    text: "Confirmez-vous la validation ?",
                    showCancelButton: true,
                    confirmButtonText: "{{ 'General.Yes'|trans }}",
                    cancelButtonText: "{{ 'General.No'|trans }}"
                }).then(function (result) {
                    if (result.value) {
                        $(location).attr('href', url);
                    }
                });
                {% endif %}

            });
            $('.commercialQuoteRequestLineRemoveButton').on('click', function () {
                quoteLineId = $(this).data('id');
                var url = "{{ path("paprec_quoteRequest_removeLine", {id: quoteRequest.id, quoteLineId: 'quoteLineIdTmp'}) }}".replace('quoteLineIdTmp', quoteLineId);


                Swal.fire({
                    title: "{{ 'General.Confirm'|trans }}",
                    text: "{{ 'General.Confirm-remove'|trans }}",
                    showCancelButton: true,
                    confirmButtonText: "{{ 'General.Yes'|trans }}",
                    cancelButtonText: "{{ 'General.No'|trans }}"
                }).then(function (result) {
                    if (result.value) {
                        $(location).attr('href', url);
                    }
                });

            });

            $('.send-generated-contract-button').on('click', function () {
                if (!$(this).hasClass('btn-disabled')) {
                    var url = "{{ path("paprec_quoteRequest_sendGeneratedContract", {id: quoteRequest.id}) }}";

                    Swal.fire({
                        title: "{{ 'General.Confirm'|trans }}",
                        text: "{{ 'Commercial.QuoteRequest.Confirm-send-contract'|trans }}",
                        showCancelButton: true,
                        confirmButtonText: "{{ 'General.Yes'|trans }}",
                        cancelButtonText: "{{ 'General.No'|trans }}"
                    }).then(function (result) {
                        if (result.value) {
                            $(location).attr('href', url);
                        }
                    });
                }
            });

            $('#formAddQuoteRequestFileSubmitButton').on('click', function () {
                $('#formAddQuoteRequestFile').submit();
            });

            $('.commercialQuoteRequestFileRemoveButton').on('click', function () {
                var quoteRequestFileId = $(this).data('id');
                var url = "{{ path("paprec_quote_request_remove_quote_request_file", {quoteRequestId: quoteRequest.id, quoteRequestFileId: '0000'}) }}".replace('0000', quoteRequestFileId);

                Swal.fire({
                    title: "{{ 'General.Confirm'|trans }}",
                    text: "{{ 'General.Confirm-remove'|trans }}",
                    showCancelButton: true,
                    confirmButtonText: "{{ 'General.Yes'|trans }}",
                    cancelButtonText: "{{ 'General.No'|trans }}"
                }).then(function (result) {
                    if (result.value) {
                        $(location).attr('href', url);
                    }
                });

            });

            $('.quote-request-add-mission-sheet-button').on('click', function () {
                var url = $(this).data('url');

                Swal.fire({
                    title: "{{ 'General.Confirm'|trans }}",
                    text: "{{ 'Commercial.QuoteRequest.MissionSheet.Confirm-add'|trans }}",
                    showCancelButton: true,
                    confirmButtonText: "{{ 'General.Yes'|trans }}",
                    cancelButtonText: "{{ 'General.No'|trans }}"
                }).then(function (result) {
                    if (result.value) {
                        $(location).attr('href', url);
                    }
                });

            });

        });
